# Copy to .env and fill the values before deploying (do NOT commit secrets)
# Postgres connection string used by Prisma
DATABASE_URL=postgresql://USER:PASSWORD@HOST:PORT/DATABASE

# App config
NODE_ENV=production
PORT=3000

# JWT secrets
JWT_SECRET=replace_with_a_long_random_string
JWT_REFRESH_SECRET=replace_with_a_long_random_string
